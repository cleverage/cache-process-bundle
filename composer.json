{
    "name": "cleverage/cache-process-bundle",
    "description": "Dedicated bundle for Cache dependencies for the Process Bundle",
    "keywords": [
        "process",
        "task",
        "etl",
        "transformation",
        "import",
        "export",
        "cache"
    ],
    "homepage": "https://github.com/cleverage/cache-process-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Joubert",
            "email": "njoubert@clever-age.com",
            "role": "Lead Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleverAge\\CacheProcessBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CleverAge\\CacheProcessBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "cleverage/process-bundle": "^4.0",
        "symfony/cache": "^6.4|^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "phpstan/extension-installer": "*",
        "phpstan/phpstan": "*",
        "phpstan/phpstan-symfony": "*",
        "phpunit/phpunit": "*",
        "rector/rector": "*",
        "roave/security-advisories": "dev-latest",
        "symfony/test-pack": "^1.1"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    }
}
